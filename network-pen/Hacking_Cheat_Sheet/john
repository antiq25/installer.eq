#To show the types of passwords that John can crack with crack speed (in cracks/second)john --test#To use your own word list (the Rockyou list is suggested)john --wordlist=[filename] [passwordfile]#To show your results after running john (shows ~/.john/john.pot)john --show#To restore an interrupted john sessionjohn --restore# ./john --show /ksanchez/password.txt# ./john –format=raw-md5 /root/passhashes.txt# ./john /root/hash_file.txt --format=nt2 -user=Administrator